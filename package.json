{
  "name": "ableton-tools",
  "private": true,
  "version": "1.0.0",
  "description": "An Ableton toolset package",
  "main": "index.js",
  "scripts": {
    "cz": "git-cz",
    "bootstrap": "npx lerna bootstrap --hoist",
    "prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json|md)\"",
    "docs": "typedoc --plugin typedoc-plugin-markdown",
    "test": "lerna run test --stream",
    "build": "lerna run build --stream",
    "publish": "lerna publish --yes",
    "canary": "lerna publish --canary --preid beta --yes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janbiasi/ableton-tools.git"
  },
  "keywords": [
    "ableton",
    "cli",
    "tools",
    "ableton",
    "tools",
    "m4l",
    "cycling74"
  ],
  "author": "Jan Biasi <biasijan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janbiasi/ableton-tools/issues"
  },
  "homepage": "https://github.com/janbiasi/ableton-tools#readme",
  "devDependencies": {
    "@types/fs-extra": "8.0.1",
    "@types/node": "13.7.1",
    "cz-conventional-changelog": "3.1.0",
    "lerna": "3.20.2",
    "prettier": "1.19.1",
    "typedoc": "0.16.10",
    "typedoc-plugin-markdown": "2.2.16",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "fs-extra": "8.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
